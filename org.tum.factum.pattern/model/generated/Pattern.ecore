    <eStructuralFeatures xsi:type="ecore:EReference" name="archConstraints" eType="#//AC"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="acFunction" eType="#//acExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="SubPattern" upperBound="-1"
        eType="#//Pattern" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ComponentType">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
